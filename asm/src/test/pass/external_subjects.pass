Process ExternalSubjectsA {
    Subject A {
        StartSubject := true

        Macro Main {
            START: Send ["a" to B] -> END
        }
    }

    InterfaceSubject B is Y in ExternalSubjectsB
}

Process ExternalSubjectsB {
    InterfaceSubject X is A in ExternalSubjectsA

    Subject Y {
        Macro Main {
            START: Receive ["a" from X] (auto) -> END
        }
    }
}
