Process Transitions2_SendSimple {
    Data {
        "agents" -> {
            "A" -> {"foo"},
            "B" -> {"bar"}
        }
    }

    Subject A {
        StartSubject := true

        Macro Main {
            START: "VarMan" ("storeData", "pingContent", "Ping!") -> Senden

            Senden: Send ["ping" to B with content of "pingContent"] -> END
        }
    }

    Subject B {
        InputPool := 2

        Macro Main {
            START: Receive ["ping" from A] (auto) -> END
        }
    }
}
